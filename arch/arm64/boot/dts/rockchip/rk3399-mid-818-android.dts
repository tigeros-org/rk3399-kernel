	vcc_phy: vcc-phy-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc_phy";
		regulator-always-on;
		regulator-boot-on;
	};
	rk_key: rockchip-key{
		compatible = "rockchip,key";
		status = "okay";
	
		io-channels = <&saradc 1>;
	
		vol-up-key {
			linux,code = <114>;
			label = "volume down";
			rockchip,adc_value = <1>;
		};
	
		vol-down-key {
			linux,code = <115>;
			label = "volume up";
			rockchip,adc_value = <170>;
		};
	
		power-key {
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
			linux,code = <116>;
			label = "power";
			gpio-key,wakeup;
		};
	};

